    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "eslint": "^8.39.0",
    "nodemon": "^2.0.22",
    "typescript": "^5.0.4",
    "vitest": "^0.30.1"
  },
  "dependencies": {
    "@octokit/rest": "^19.0.7",
    "axios": "^1.4.0",
    "chrono-node": "^2.6.3",
