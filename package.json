{
  "name": "amber",
  "version": "0.1.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --transpileOnly --exec ts-node src/index.ts",
    "start": "ts-node --transpileOnly src/index.ts",
    "build": "swc src -d dist --copy-files --delete-dir-on-start",
    "test": "vitest run",
    "test:watch": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "eslint src --ext .ts,.tsx",
    "format": "prettier --write \"src/**/*.ts\"",
    "check-types": "tsc --noEmit",
    "ci": "npm run lint && npm run check-types && npm run test",
    "ci:coverage": "npm run ci && npm run coverage"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.56",
    "@types/node-cron": "^3.0.7",
    "@types/node-schedule": "^2.1.0",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "eslint": "^8.39.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4",
    "vitest": "^0.30.1"
  },
  "dependencies": {
    "@octokit/rest": "^19.0.7",
    "@swc/helpers": "^0.5.1",
    "axios": "^1.4.0",
    "chrono-node": "^2.6.3",
    "discord.js": "^14.13.0",
    "dotenv": "^16.0.3",
    "eventemitter2": "^6.4.9",
    "hanako": "^0.3.0",
    "intent-utterance-generator": "^0.0.2",
    "mongoose": "^7.1.0",
    "node-cron": "^3.0.2",
    "node-nlp": "^4.26.1",
    "node-schedule": "^2.1.1",
    "picocolors": "^1.0.0",
    "ponaserv": "^1.1.0",
    "weather-js": "^2.0.0"
  }
}
